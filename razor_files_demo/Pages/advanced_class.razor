@page "/Advancedclass"
@using razor_files_demo.Pages.ContactComponents

@foreach (var contact in contacts)
{
    <ContactComponents01 FirstName="contacts.FirstName" LastName="contacts.LastName" />
}
@code{
    private List<Contacts> contacts;
    protected override void OnInitialized()
    {
        base.OnInitialized();
        contacts = new List<Contacts>
        {
            new Contacts { FirstName = "John", LastName = "Doe", Email = "john.doe@example.com" },
            new Contacts { FirstName = "Jane", LastName = "Smith", Email = "jane.smith@example.com" },
            new Contacts { FirstName = "Michael", LastName = "Johnson", Email = "michael.johnson@example.com" },
            new Contacts { FirstName = "Emily", LastName = "Davis", Email = "emily.davis@example.com" },
            new Contacts { FirstName = "David", LastName = "Wilson", Email = "david.wilson@example.com" }
        };
    }
}